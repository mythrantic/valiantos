{
  "name": "ValiantOS",
  "short_name": "ValiantOS",
  "description": "ValiantOS is a web app designed to resemble a desktop environment using SvelteKit.",
  "start_url": "/",
  "id": "/",
  "display_override": ["window-controls-overlay","fullscreen", "minimal-ui"],
  "display": "standalone",
  "background_color": "#25133F",
  "theme_color": "#25133F",
  "orientation": "any",
  "lang": "en-US",
  "scope": "/",
  "dir": "auto",
  "categories": ["utilities", "productivity"],
  "protocol_handlers": [
    {
        "protocol": "mailto",
        "url": "/newEmail?to=%s"
    }
  ],
  "shortcuts": [
    {
        "name": "Play Later",
        "description": "View the list of podcasts you saved for later",
        "url": "/play-later",
        "icons": [
            {
                "src": "/icons/play-later.png",
                "type": "image/png",
                "purpose": "any"
            }
        ]
    },
    {
        "name": "Subscriptions",
        "description": "View the list of podcasts available in your subscription",
        "url": "/subscriptions?sort=desc",
        "icons": [
            {
                "src": "/icons/subscriptions.webp",
                "type": "image/webp",
                "purpose": "any"
            }
        ]
    }
],
